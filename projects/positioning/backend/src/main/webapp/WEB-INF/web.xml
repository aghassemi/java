<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>BluetoothProximityServlet</servlet-name>
        <servlet-class>io.v.positioning.BluetoothProximityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BluetoothProximityServlet</servlet-name>
        <url-pattern>/bluetooth</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MyGPSLocationServlet</servlet-name>
        <servlet-class>io.v.positioning.GpsLocationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyGPSLocationServlet</servlet-name>
        <url-pattern>/gps</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>